PREFIX ex: <http://example.org/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
SELECT  ?person ?model
WHERE{ 
  {?person a ex:Person;
           ex:drives ?c.
  }
  UNION{
    SERVICE <http://localhost:3030/foaf_person>{
      ?person a foaf:Person;
              ex:drives ?c.
    }
  }
  ?c ex:model ?model.
}
